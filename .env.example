# Google OAuth 配置
VITE_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# n8n 配置
VITE_N8N_WEBHOOK_URL=https://your-n8n-domain.com/webhook
VITE_N8N_TIMEOUT=30000

# 後端 API 配置
VITE_BACKEND_URL=http://localhost:3001

# 應用配置
VITE_APP_TITLE=LeadIO SEO Analysis
VITE_APP_DESCRIPTION=AI-driven SEO analysis platform
VITE_ENABLE_LOGGING=true

# 功能開關
VITE_ENABLE_REPORTS=true
VITE_ENABLE_EMAIL_REPORTS=true
VITE_ENABLE_AUTO_REFRESH=true